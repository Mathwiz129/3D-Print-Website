# Local Development Environment Variables
# Copy this file to .env and uncomment the lines you want to use

# Firebase Configuration (optional - will use hardcoded values if not set)
# FIREBASE_API_KEY=your_firebase_api_key_here
# FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
# FIREBASE_PROJECT_ID=your_project_id
# FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
# FIREBASE_MESSAGING_SENDER_ID=your_sender_id
# FIREBASE_APP_ID=your_app_id
# FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google Sheets API (optional - will use hardcoded values if not set)
# GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key_here

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=1
PORT=5000

# JWT Secret (for local development only)
JWT_SECRET=your-local-development-secret-key-change-this 